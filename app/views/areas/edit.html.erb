<h1>Editing area</h1>

<% form_for(@area) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :descricao %><br />
    <%= f.text_field :descricao %>
  </p>
  <p>
    <%= f.label :cidade_id %><br />
    <%= f.text_field :cidade_id %>
  </p>
  <p>
    <%= f.label :roteiro_id %><br />
    <%= f.text_field :roteiro_id %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @area %> |
<%= link_to 'Back', areas_path %>